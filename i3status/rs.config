theme = "modern"
icons = "awesome"

[[block]]
block = "focused_window"
max_width = 21

[[block]]
block = "memory"
display_type = "memory"
format_mem = "{Mup}%"
format_swap = "{SUp}%"
interval = 3

[[block]]
block = "cpu"
interval = 3

[[block]]
block = "sound"

[[block]]
block = "time"
interval = 5
format = " %H:%M:%S / %d.%m.%Y"

[[block]]
block = "custom"
command = "printf '\uF011'"
on_click = "zexit"
