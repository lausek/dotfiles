theme = "modern"
icons = "awesome"

[[block]]
block = "memory"
display_type = "memory"
format_mem = "{Mup}%"
format_swap = "{SUp}%"

[[block]]
block = "cpu"
interval = 1

[[block]]
block = "load"
interval = 1
format = "{1m}"

[[block]]
block = "sound"

[[block]]
block = "time"
interval = 60
format = " %H:%M:%S / %d.%m.%Y"

[[block]]
block = "custom"
command = "printf '\uF011'"
on_click = "zexit"
